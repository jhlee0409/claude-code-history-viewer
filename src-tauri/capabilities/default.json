{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "dialog:default",
    "process:allow-restart",
    "updater:default",
    "updater:allow-check",
    "store:allow-load",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save"
  ]
}
